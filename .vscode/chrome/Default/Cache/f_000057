        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-49982725-2', 'auto');
        ga('send', 'pageview');

        //Tracking for Quic Demo Video Link
        $(document).ready(function () {
            $(".js-modal-btn").click(function (e) {
                ga('send', 'event', 'Video', 'Play', 'Quick Demo');
            });
        });
    </script>


</body>
</html>
